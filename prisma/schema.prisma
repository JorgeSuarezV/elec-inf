generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://postgres:postgres@ec2-34-201-64-143.compute-1.amazonaws.com:5432/ei"
}

model User {
  id           Int     @id @default(autoincrement())
  rfid         String  @unique
  name         String?
  serveCounter Int     @default(0)
  temperature  Int     @default(20)
}
